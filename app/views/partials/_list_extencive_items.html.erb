<table>
  <thead>
    <tr>
      <th>i_name</th>
      <th>ui_price</th>
      <th>link</th>
    </tr>
  </thead>

  <tbody>
    <% @expensive_items.each do |item| %>
      <tr>
        <td><%= item.i_name %></td>
        <td><%= item.ui_price %></td>
        <td><%= link_to 'Link', item.link %></td>
      </tr>
    <% end %>
  </tbody>
</table>
